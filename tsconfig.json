    "strict": true,
    "module": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2018", "ES2019.Object"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
